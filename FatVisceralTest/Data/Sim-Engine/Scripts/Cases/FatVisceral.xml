          <RemapFacesMinTexCoord>0.9, 0.0, -1</RemapFacesMinTexCoord>
          <RemapFacesMaxTexCoord>1.0, 0.1, -1</RemapFacesMaxTexCoord>
          <UseDynamicSphereTree>1</UseDynamicSphereTree>
          <UseSurroundGrab>1</UseSurroundGrab>
          <AdjacentParticlesUpdateDepth>2</AdjacentParticlesUpdateDepth>
          <RemoveFragmentGravity>0,-800,0</RemoveFragmentGravity>
          <!--The PhysicsModifier (FEM) is only relevant if mu or lambda are greater than zero.-->
