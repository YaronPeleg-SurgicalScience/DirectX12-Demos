          <TerrainKs>5500.0</TerrainKs>
          <ZeroBadQuality>1</ZeroBadQuality>
          <RemoveReversedTetras>1</RemoveReversedTetras>
          <SmoothSurface>1</SmoothSurface>
          <UseGrabEdgesForCut>1</UseGrabEdgesForCut>
          <!--When this (WeldEdgeSqThresh) is large, the retesselated edges will be large (hence bigger cuts than the scisors).-->
          <WeldEdgeSqThresh>0.5</WeldEdgeSqThresh>
          <WeldToFaceThresh>-1</WeldToFaceThresh>
          <!--sqr edge length (was 20.0, 0.7 in some other file (lobectomy?))-->
          <EdgeTesselationLenThresh>16</EdgeTesselationLenThresh>
          <UseMultiCutTri>1</UseMultiCutTri>
          <MarkInnerVerts>1</MarkInnerVerts>
          <ReUseEmptyElements>1</ReUseEmptyElements>
          <JustRemoveWhenGrabbed>0</JustRemoveWhenGrabbed>
          <MuRepairVal>0</MuRepairVal>
          <LambdaRepairVal>0</LambdaRepairVal>
          <KsRepairVal>5000</KsRepairVal>
          <SquashEnable>1</SquashEnable>
          <SquashMode>2</SquashMode>
          <SquashPlaneRadius>10.0</SquashPlaneRadius>
          <SquashSphereRadius>10.0</SquashSphereRadius>
          <SquashScale>0.7</SquashScale>
          <RelativeCutBaseSeperation>0.05</RelativeCutBaseSeperation>
          <__NewParticleGravity>0,-1,0</__NewParticleGravity>
          <CuttingSupported>1</CuttingSupported>
          <CollideBothSides>1</CollideBothSides>
          <CutOriginShift>5</CutOriginShift>
          <__RemapFacesMinTexCoord>0.6337890625, 0.6337890625, 0.0</__RemapFacesMinTexCoord>
          <__RemapFacesMaxTexCoord>1.0, 1.0, 0.0</__RemapFacesMaxTexCoord>
          <RenderToDepthTexture>1</RenderToDepthTexture>
          <SortNewPointsToGroups>1</SortNewPointsToGroups>
          <VertexUpdaterEnabled>1</VertexUpdaterEnabled>
          <ConnectivityUtil>1</ConnectivityUtil>
          <GrabStiffCoef>0</GrabStiffCoef>
          <EnforceManifold>0</EnforceManifold>
